    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "history": "^4.10.1",
    "if-env": "^1.0.4",
    "md5": "^2.2.1",
    "mongoose": "^5.3.16",
    "mysql2": "^2.0.0",
    "react-apexcharts": "^1.3.3",
    "react-router-dom": "^5.1.2",
    "react-select": "^3.0.8",
    "sequelize": "^5.21.2",
    "sequelize-mysql-timestamp": "^1.3.0"
